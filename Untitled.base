views:
  - type: table
    name: Table
    filters:
      and:
        - '!file.name.contains("m4a")'
        - '!file.name.contains("pdf")'
        - '!file.name.contains("png")'
    order:
      - file.name
      - file.folder
      - file.size
    sort:
      - property: file.size
        direction: DESC
    columnSize:
      file.name: 448
