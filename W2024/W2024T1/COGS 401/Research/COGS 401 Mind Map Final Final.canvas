      "width": 262,
      "height": 60
    },
    {
      "id": "2a1b185e16a5c267",
      "type": "text",
      "text": "**Understanding through visualization**",
      "styleAttributes": {
        "shape": "pill"
      },
      "x": -104,
      "y": 19,
      "width": 228,
      "height": 60
    },
    {
      "id": "267eabde6814190d",
      "type": "text",
      "text": "**\"Mirror stage\"** ",
      "styleAttributes": {
        "shape": "pill"
      },
      "x": -150,
